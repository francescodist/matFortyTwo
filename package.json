{
    "name": "mat-forty-two",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "test:single-run": "ng test --watch=false",
        "lint:check": "ng lint",
        "lint:fix": "ng lint --fix",
        "e2e": "ng e2e",
        "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
        "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
        "generate:navPage": "scripty",
        "generate:rootPage": "scripty"
    },
    "husky": {
        "hooks": {
            "pre-commit": "run-s test:single-run format:fix format:check lint:fix lint:check"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^7.2.14",
        "@angular/cdk": "^7.3.7",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/flex-layout": "^7.0.0-beta.24",
        "@angular/forms": "~7.2.0",
        "@angular/material": "^7.3.7",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "core-js": "^2.5.4",
        "hammerjs": "^2.0.8",
        "rxjs": "~6.3.3",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular/cli": "~7.3.8",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "husky": "^2.1.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.17.0",
        "protractor": "~5.4.0",
        "scripty": "^1.9.1",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "~3.2.2"
    }
}
